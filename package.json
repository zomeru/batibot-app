{
  "name": "batibot",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:reset": "react-native start -- --reset-cache",
    "android:clean": "concurrently \"yarn run clean\" \"yarn run android:reset\"",
    "clean": "rm -rf android/build && rm -rf android/app/build && rm -rf android/.gradle && cd android && ./gradlew clean && cd .. && watchman watch-del-all",
    "android:build:apk": "concurrently \"yarn run clean\" \"yarn run release:android:apk\"",
    "android:build:bundle": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "release:android:apk": "cd android && ./gradlew assembleRelease",
    "android:assets": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clear:duplicate": "rm -rf ./android/app/src/main/res/drawable-* && rm -rf ./android/app/src/main/res/raw",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@supabase/supabase-js": "^2.24.0",
    "base-64": "^1.0.0",
    "nativewind": "^2.0.11",
    "openai": "^3.2.1",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.10.2",
    "react-native-inappbrowser-reborn": "^3.7.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-reanimated": "2.17.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-native-toast-message": "^2.1.6",
    "react-native-typewriter": "^0.7.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native-typewriter": "^0.7.0",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-universe": "^11.2.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.8.8",
    "react-native-dotenv": "^3.4.8",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{js,ts,tsx,css,md}": "prettier --write"
  }
}

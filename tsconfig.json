{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "~components/*": ["components/*"],
      "~utils/*": ["utils/*"],
      "~assets/*": ["assets/*"],
      "~contexts/*": ["contexts/*"],
      "~hooks/*": ["hooks/*"],
      "~constants/*": ["constants/*"],
      "~services/*": ["services/*"]
    },
    "module": "ES6"
  }
}
